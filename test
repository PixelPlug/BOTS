var nrImagem = 0;
var imagens = [];
var refrescar = 1; // mudar imagem de 1 em 1 segundo

// colocar aqui todas as imagens, seguindo a ordem numÃ©rica
imagens[0] = "http://i.imgur.com/gqdMdaz.gif";
imagens[1] = "http://www.w3schools.com/images/w3html.gif";
imagens[2] = "http://www.javascriptkit.com/complete/images/tn00607a.gif";
//...

function rodarImagens() {
   document.images["dj-console"].src = imagens[nrImagem++];

   if (nrImagem >= imagens.length)
      nrImagem = 0;
                       
   setTimeout("rodarImagens()", refrescar * 1000);  
}

rodarImagens();
